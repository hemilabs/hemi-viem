{
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitReturns": true,
    "resolveJsonModule": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "useUnknownInCatchVariables": false
  },
  "exclude": ["node_modules"],
  "include": ["sr*/**/*.ts"]
}
